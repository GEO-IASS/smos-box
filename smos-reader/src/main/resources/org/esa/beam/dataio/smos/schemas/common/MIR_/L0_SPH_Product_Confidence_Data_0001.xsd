<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://213.170.46.150/smos/schemas" targetNamespace="http://213.170.46.150/smos/schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0001">
	<xs:include schemaLocation="../common/Basic_Types_0001.xsd"/>
	<xs:complexType name="percentage_integer_s06d_Unit_Type">
		<xs:simpleContent>
			<xs:extension base="integer_s06d_Type">
				<xs:attribute name="unit" type="string_Type" default="10-2%"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="L0_Pass_Confidence_Data_Type">
		<xs:sequence>
			<xs:element name="Pass_Start_Time" type="string_date_30_Type" default="UTC=2007-01-01T00:00:00.000000"/>
			<xs:element name="Pass_Stop_Time" type="string_date_30_Type" default="UTC=2007-01-01T00:00:00.000000"/>
			<xs:element name="N_Missing_24packet_Blocks" type="integer_s06d_Type" default="+00000"/>
			<xs:element name="N_RS_Corrected_Bytes" type="integer_s06d_Type" default="+00000"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="L0_Product_Confidence_Data_Type">
		<xs:sequence>
			<xs:element name="N_Valid_Packets" type="integer_s06d_Type" default="+00000"/>
			<xs:element name="N_Missing_Packets" type="integer_s06d_Type" default="+00000"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="L0_Product_Derived_Confidence_Data_Type">
		<xs:sequence>
			<xs:element name="Missing_ISP_Threshold" type="percentage_integer_s06d_Unit_Type" default="+00000"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
