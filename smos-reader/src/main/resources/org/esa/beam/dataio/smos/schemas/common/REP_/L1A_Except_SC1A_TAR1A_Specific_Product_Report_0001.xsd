<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://213.170.46.150/smos/schemas" targetNamespace="http://213.170.46.150/smos/schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0001">
	<xs:include schemaLocation="../common/Basic_Types_0001.xsd"/>
	<xs:complexType name="Calibration_info_Type">
		<xs:sequence>
			<xs:element name="Number_of_Calib_Sequences" type="integer_03d_Type" default="000"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Raw_Correlations_Processor_Type">
		<xs:sequence>
			<xs:element name="Antennae_pair" type="string_Type" default="x"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Elements_out_of_range_Type">
		<xs:sequence>
			<xs:element name="Element_ID" type="string_Type" default="x"/>
		</xs:sequence>
	</xs:complexType>	
	<xs:complexType name="List_Elements_out_of_ranges_Type">
		<xs:sequence>
			<xs:element name="Elements_out_of_range" type="Elements_out_of_range_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="count" type="unsigned_Integer_hd_Type" default="0"/>
	</xs:complexType>
	<xs:complexType name="Specific_Product_Report_Type">
		<xs:sequence>
			<xs:element name="Number_Downlinked_Packets" type="integer_04d_Type" default="0000"/>
			<xs:element name="Number_Missing_Packets" type="integer_04d_Type" default="0000"/>
			<xs:element name="Number_Corrupted_Packets" type="integer_04d_Type" default="0000"/>
