<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://213.170.46.150/smos/schemas" targetNamespace="http://213.170.46.150/smos/schemas" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0001">
	<xs:include schemaLocation="../common/Basic_Types_0001.xsd"/>
	<xs:complexType name="percent_integer_s06d_Unit_Type">
		<xs:simpleContent>
			<xs:extension base="integer_s06d_Type">
				<xs:attribute name="unit" type="string_Type" fixed="10-2 %"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="epochs_unsigned_Integer_04u_Unit_Type">
		<xs:simpleContent>
			<xs:extension base="integer_04u_Type">
				<xs:attribute name="unit" type="string_Type" fixed="epochs"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="Quality_Information_A_Type">
		<xs:sequence>
			<xs:element name="L1A_Err_Flag" type="integer_01d_Flag_Type" default="0"/>
			<xs:element name="L1A_Quality" type="percent_integer_s06d_Unit_Type" default="+00000"/>
			<xs:element name="L1A_Err_Threshold" type="percent_integer_s06d_Unit_Type" default="+00000"/>
			<xs:element name="L1A_Missing_Epochs" type="epochs_unsigned_Integer_04u_Unit_Type" default="0000"/>
			<xs:element name="L1A_Maximum_Gap" type="epochs_unsigned_Integer_04u_Unit_Type" default="0000"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
