<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <!--
SNAP autoupdate extensions
-->
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="smos_update_center.instance">
                <attr name="displayName" stringvalue="SMOS-Box Update Center"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate"
                      methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" urlvalue="http://step.esa.int/updatecenter/2.0/smos/updates.xml.gz"/>
            </file>
        </folder>
    </folder>

</filesystem>